{
  "name": "gypcrete",
  "version": "0.2.0",
  "description": "iCHEF web components library, built with React.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/iCHEF/gypcrete.git"
  },
  "contributors": [
    "CJies Tan (https://github.com/cjies)",
    "Teng-hao Chang (https://github.com/zhusee2)"
  ],
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "^6.9.1",
    "npm": "^3.10.10"
  },
  "scripts": {
    "start": "BABEL_ENV=server webpack-dev-server --config ./config/webpack.dev.js",
    "build": "npm run build:dist && npm run build:lib && npm run build:es5",
    "build:dist": "webpack -p --config ./config/webpack.prod.js --color",
    "build:lib": "BABEL_ENV=lib babel src --out-dir lib",
    "build:es5": "BABEL_ENV=es5 babel src --out-dir es5",
    "lint": "eslint ./ && echo \"eslint: no lint errors\""
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-strip-css-imports": "zhusee2/babel-plugin-strip-css-imports",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-ichef": "^1.1.2",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.0.0"
  }
}
